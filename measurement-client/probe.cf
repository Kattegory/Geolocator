[probe]
# URL of the list of addresses to probe.
landmark_url=https://research.owlfolio.org/active-geo/api/1/landmark-list
# URL to push results back to.
results_url=https://research.owlfolio.org/active-geo/api/1/probe-results
# Time between successive probes (seconds)
spacing=0.1
# Number of concurrent probes to perform
parallel=10
# Connection timeout
timeout=10
# Number of times to probe each landmark
n_probes=10
